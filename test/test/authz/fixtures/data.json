{

    "env_default": {
        "env": {
            "now": "2020-02-02T12:34:56Z"
        }
    },

    "op_capi_create_payment_resource": {
        "capi": {
            "op": {
                "id": "CreatePaymentResource",
                "party": {"id": "PARTY"}
            }
        }
    },

    "op_capi_create_invoice": {
        "capi": {
            "op": {
                "id": "CreateInvoice",
                "invoice": {"id": "INVOICE"},
                "shop": {"id": "SHOP"},
                "party": {"id": "PARTY"}
            }
        }
    },

    "op_capi_get_invoice": {
        "capi": {
            "op": {
                "id": "GetInvoiceByID",
                "invoice": { "id": "INVOICE" },
                "shop": { "id": "SHOP" },
                "party": { "id": "PARTY" }
            }
        }
    },

    "op_capi_create_refund": {
        "capi": {
            "op": {
                "id": "CreateRefund",
                "invoice": {"id": "INVOICE"},
                "payment": {"id": "PAYMENT"},
                "shop": {"id": "SHOP"},
                "party": {"id": "PARTY"}
            }
        }
    },


    "op_capi_get_invoice_2": {
        "capi": {
            "op": {
                "id": "GetInvoiceByID",
                "invoice": { "id": "INVOICE_2" },
